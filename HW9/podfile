  pod 'Eureka' 
  pod 'GooglePlacePicker'
  pod 'Firebase/Database'

  # Pods for GeoCalculator

  target 'GeoCalculatorTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'GeoCalculatorUITests' do
    inherit! :search_paths
    # Pods for testing
  end


end
